{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/submitReview.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/submitReview",
      "dest": "api/submitReview.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
